{
  "name": "playradar",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "studio": "set PRISMA_HIDE_UPDATE_MESSAGE=1 && npx prisma studio --schema prisma/schema.prisma",
    "docker": "docker-compose up",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "storybook": "storybook dev -p 6006",
    "prepare": "cd .. && husky install playradar/.husky",
    "postinstall": "prisma generate"
  },
  "proxy": {
    "/api/*": {
      "target": "https://api.rawg.io/api",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api": ""
      }
    }
  },
